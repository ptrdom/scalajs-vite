$ absent client/vite/yarn.lock
> set client/scalaJSStage := org.scalajs.sbtplugin.Stage.FastOpt
> server/bgRun
> html 8080
$ exists client/vite/yarn.lock
-> exit

$ delete client/vite/yarn.lock
> clean
